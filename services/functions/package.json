{
  "name": "functions",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "build": "rm -rf lib && tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy:default": "firebase deploy --only functions --project default",
    "logs": "firebase functions:log",
    "test": "env-cmd -f .env.local vitest run --pool threads",
    "emulators:exec:test": "pnpm -w build:shared && pnpm -w tsx scripts/src/emulatorsExec.ts --only auth,firestore --project demo-functions --script 'pnpm --dir services/functions test'"
  },
  "engines": {
    "node": "22"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/functions-framework": "^4.0.1",
    "@google/genai": "^1.34.0",
    "@local/admin-shared": "workspace:^",
    "@local/shared": "workspace:^",
    "firebase-admin": "^13.6.0",
    "firebase-functions": "^6.6.0",
    "google-auth-library": "^10.5.0",
    "googleapis": "^169.0.0",
    "p-limit": "^7.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@local/test-shared": "workspace:^",
    "@vitest/eslint-plugin": "^1.4.3",
    "env-cmd": "^11.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "firebase-functions-test": "^3.4.1",
    "globals": "^16.5.0",
    "pnpm": "10.23.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vitest": "^4.0.10"
  },
  "packageManager": "pnpm@10.23.0",
  "private": true
}
